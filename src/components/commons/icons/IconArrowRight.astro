---
import IconKeenIcon from "./IconKeenIcon.astro";

interface Props {
  classNameContainer?: string | undefined;
  className?: string | undefined;
  color?: string | undefined;
  size?: string | undefined;
}

const {
  classNameContainer,
  className,
  color,
  size,
} = Astro.props;
---

<!-- ICON - ARROW RIGHT -->
<IconKeenIcon
  name="arrow-right"
  paths={2}
  classNameContainer={classNameContainer}
  className={className}
  color={color}
  size={size} />
